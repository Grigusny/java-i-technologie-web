package Lista4;

import java.util.Random;
import java.util.Scanner;
public class Zad1 {
public static void main(String[] args) {
int [] tab  = new int[30];	
int [] tab2 = new int[30];	
Random rand = new Random();
for(int i =0; i<30; i++) 
	tab[i] = rand.nextInt(51)+99;					
	
	for(int i=0; i<30; i++)	
		System.out.println("tablica["+i+"] = "+tab[i]);
	System.arraycopy(tab,0,tab2,0,tab.length);
	for(int i = 0; i<30;i++) 
								
		System.out.println("tablica skopiowana["+i+"] = "+tab2[i]);
}
}
